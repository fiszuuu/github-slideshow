#include <stdio.h>

int main ()
{
    const int n=5;
    int tab[5]={13, 515, 53, 6 , 1};
    int i, j, a;
    for (i=0; i<n; ++i)
    {
        for (j=0; j<n; ++j)

        {
            if (tab[j]>tab[j+1])
            {
                a=tab[j];
                tab[j]=tab[j+1];
                tab[j+1]=a;
            }
        }
    }
    for (i=0; i<n; ++i)
    {
        printf ("%d ", tab[i]);
    }
    return 0;
}
