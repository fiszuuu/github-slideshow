#include <stdio.h>


int main ()
{
    int a , b, c;
    scanf("%d %d %d", &a, &b, &c);
    switch (a)
    {
        case 1:
            printf ("%d\n", c+b);
        break;
        case 2:
            printf ("%d\n", b-c);
        break;
        case 3:
            printf ("%d\n", c*b);
        break;
        case 4:
            printf ("%d\n", b/c);
        break;
        default:
            printf  ("nie ma takiej operacji");
        break;
    }
    return 0;
}
